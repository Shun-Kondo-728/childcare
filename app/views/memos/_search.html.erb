<%= search_form_for @q, url: memo_search_path do |f| %>
    <div class="col-md-9">
      <div class='form-group'>
        <%= f.search_field :name_cont, placeholder: "キーワード検索", class: 'form-control font-awesome' %>
      </div>
    </div>
    <div class="col-md-9">
      <div class='form-group'>
        <%= f.submit nil, class: 'btn btn-primary' %>
      </div>
    </div>
<% end %>