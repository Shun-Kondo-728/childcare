<%= link_to '<<戻る', root_path, class: "back-link" %>
<%= link_to 'メモする', new_calendar_path, class: "memo-go" %>

<%= month_calendar events: @calendars do |date, calendars| %>
  <%= date.day %>

  <% calendars.each do |calendar| %>
    <div>
      <%= link_to calendar.name, calendar %>
    </div>
  <% end %>
<% end %>
